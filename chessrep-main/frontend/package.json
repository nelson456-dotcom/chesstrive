{
  "name": "chess-analysis-board",
  "version": "1.0.0",
  "description": "Production-quality interactive chess analysis board",
  "private": true,
  "dependencies": {
    "@mliebelt/pgn-parser": "^1.4.18",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.4",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "chess-moments": "^1.4.8",
    "chess.js": "^0.13.4",
    "chess.mjs": "^2.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "cm-chess": "^1.6.3",
    "cm-chessboard": "^8.10.1",
    "cm-pgn": "1.7.5",
    "cm-web-modules": "^2.5.0",
    "dnd-multi-backend": "^7.1.3",
    "framer-motion": "^10.16.0",
    "immer": "^10.0.0",
    "lucide-react": "^0.553.0",
    "modrator": "^1.2.1",
    "postcss": "^8.4.0",
    "postcss-loader": "^4.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-chessboard": "^4.7.3",
    "react-dnd-multi-backend": "^7.0.2",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^4.9.5",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.0",
    "cypress": "^13.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-typescript": "^0.0.1",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  }
}
